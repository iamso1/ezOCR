<!DOCTYPE html>
<html>
  <head>
    <title>OCR Demo</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
  </head>
  <script>
    $(document).ready(function(){
      $("#btn_submit").click(function(){
          $.post( "/ezocr", { base64Str:'data:image/png;base64,' })
            .done(function( data ) {
              $('#answer').text(data);
          });
      });
    });  
  </script>
  
  <style>
    table, th, td {
        border: 1px solid black;
    }
  </style>
  <body>
    <table>
      <tr>
        <td><label>sample image:</label></td>
        <td><image src='images/demo.png'></image></td>
      </tr>
      <tr>
        <td colspan='2'><button id='btn_submit'>submit</button></td>
      </tr>
      <tr>
        <td>Answer:</td>
        <td id='answer'></td>
      </tr>
    </table>
  </body>
</html>